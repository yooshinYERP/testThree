    					</w2:attributes>
    				</w2:span>
    			</xf:group>
    			<xf:group id="" style="" class="item" tagname="li">
    				<w2:anchor id="" outerDiv="false">
    					<xf:label></xf:label>
    				</w2:anchor>
    				<w2:span id="" label="셔틀그리드(가로)">
    					<w2:attributes>
    						<w2:data-url>/xml/pageSample/shuttle_grid_h.xml</w2:data-url>
    					</w2:attributes>
    				</w2:span>
    			</xf:group>
    			<xf:group id="" style="" class="item" tagname="li">
    				<w2:anchor id="" outerDiv="false">
    					<xf:label></xf:label>
    				</w2:anchor>
    				<w2:span id="" label="셔틀그리드(세로)">
    					<w2:attributes>
    						<w2:data-url>/xml/pageSample/shuttle_grid_v.xml</w2:data-url>
    					</w2:attributes>
    				</w2:span>
    			</xf:group>
    			<xf:group class="item" id="" style="" tagname="li">
    				<w2:anchor id="" outerDiv="false">
    					<xf:label></xf:label>
    				</w2:anchor>
    				<w2:span id="" label="팝업창">
    					<w2:attributes>
    						<w2:data-url>/xml/pageSample/popup.xml</w2:data-url>
    					</w2:attributes>
    				</w2:span>
    			</xf:group>
    			<xf:group class="item" id="" style="" tagname="li">
    				<w2:anchor id="" outerDiv="false">
    					<xf:label></xf:label>
    				</w2:anchor>
    				<w2:span id="" label="시스템메시지창">
    					<w2:attributes>
    						<w2:data-url>/xml/pageSample/alert.xml</w2:data-url>
    					</w2:attributes>
    				</w2:span>
    			</xf:group>
    		</xf:group></xf:group></xf:group></body>
</html>
